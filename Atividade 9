#include <stdio.h>
int imprime(char var, int x){
    for (int l=1; l<=x; l++){
        for (int c=1; c<=x; c++){
            printf ("%c", var);
        }
        printf("\n");
    }
    return x*x;
}

int main() {
    int quant;
    quant = imprime ('/',4);
    
    printf ("Foram impressos %d caracteres no primeiro e %d no segundo", quant, imprime('!',5) );
    return 0;
}


